<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GameImageView">
        <attr name="sapemode" format="reference">
            <enum name="vertical" value="0"></enum>
            <enum name="horizontal" value="1"></enum>
            <enum name="square" value="2"></enum>
        </attr>

    </declare-styleable>
    <declare-styleable name="TitleLine">
        <attr name="divided" format="integer"/>
        <attr name="dividedHight" format="dimension"></attr>
        <attr name="dividedColor" format="reference"></attr>
        <attr name="bg_color" format="reference"></attr>
        <attr name="bg_height" format="dimension"></attr>
    </declare-styleable>


    <declare-styleable name="ScreenShot">
        <!--<attr name="nums" format="integer"/>-->
        <attr name="itemWidth" format="dimension"></attr>
        <attr name="itemHeight" format="dimension"></attr>
        <attr name="interval" format="dimension"></attr>
    </declare-styleable>
</resources>